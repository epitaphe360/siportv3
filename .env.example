# Configuration Firebase pour l'authentification Google
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdefghijklmnop

# Configuration Supabase (Railway PostgreSQL)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Exhibitors fallback server
# Secret required in 'x-exhibitors-secret' header when calling the server endpoint
EXHIBITORS_SECRET=dev-secret
# Optional custom port for the exhibitors server (defaults to 4002)
EXHIBITORS_PORT=4002

# Frontend access to exhibitors fallback server (optional)
VITE_EXHIBITORS_SERVER_URL=http://localhost:4002
VITE_EXHIBITORS_SECRET=dev-secret
